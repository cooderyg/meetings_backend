{
  "name": "anote-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm run test:db:ensure && NODE_ENV=test jest --runInBand",
    "test:watch": "pnpm run test:db:ensure && NODE_ENV=test jest --watch --runInBand",
    "test:cov": "pnpm run test:db:ensure && NODE_ENV=test jest --coverage --runInBand",
    "test:debug": "pnpm run test:db:ensure && NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm run test:db:ensure && NODE_ENV=test jest --config ./test/jest-e2e.json",
    "test:ci": "NODE_ENV=test jest",
    "test:db:ensure": "docker ps | grep anote-postgres-test > /dev/null || pnpm run test:db:up",
    "test:db:up": "docker-compose -f docker-compose.test.yaml up -d && sleep 3",
    "test:db:down": "docker-compose -f docker-compose.test.yaml down",
    "test:db:logs": "docker-compose -f docker-compose.test.yaml logs -f postgres-test",
    "test:unit": "NODE_ENV=test jest --testPathPattern=\\.spec\\.ts$ --testPathIgnorePatterns=e2e",
    "test:integration": "NODE_ENV=test jest --config ./jest.integration.config.ts",
    "test:tc": "NODE_ENV=test jest --config ./jest.integration.config.ts",
    "test:tc:parallel": "NODE_ENV=test jest --config ./jest.integration.config.ts --maxWorkers=50%",
    "test:fast": "pnpm run test:unit",
    "test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
    "test:unit:watch": "NODE_ENV=test jest --testPathPattern=\\.spec\\.ts$ --watch --testPathIgnorePatterns=e2e",
    "test:integration:watch": "NODE_ENV=test jest --config ./jest.integration.config.ts --watch",
    "test:user": "NODE_ENV=test jest --testPathPattern=user.*\\.spec\\.ts$",
    "test:meeting": "NODE_ENV=test jest --testPathPattern=meeting.*\\.spec\\.ts$",
    "test:workspace": "NODE_ENV=test jest --testPathPattern=workspace.*\\.spec\\.ts$",
    "migration:create": "npx mikro-orm migration:create",
    "migration:up": "npx mikro-orm migration:up",
    "migration:down": "npx mikro-orm migration:down",
    "seed": "npx mikro-orm seeder:run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.858.0",
    "@aws-sdk/s3-request-presigner": "^3.858.0",
    "@google-cloud/speech": "^7.2.0",
    "@langchain/openai": "^0.6.3",
    "@mikro-orm/core": "^6.4.16",
    "@mikro-orm/nestjs": "^6.1.1",
    "@mikro-orm/postgresql": "^6.4.16",
    "@mikro-orm/reflection": "^6.4.16",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.5",
    "axios": "^1.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "langchain": "^0.3.30",
    "langsmith": "^0.3.50",
    "mp3-duration": "^1.1.0",
    "nest-winston": "^1.10.2",
    "nestjs-cls": "^6.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@mikro-orm/cli": "^6.4.16",
    "@mikro-orm/seeder": "^6.4.16",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@swc/jest": "^0.2.39",
    "@testcontainers/postgresql": "^11.7.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/pumpify": "^1.4.4",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-node-single-context": "^29.4.0",
    "prettier": "^3.4.2",
    "socket.io": "^4.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "testcontainers": "^11.7.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
